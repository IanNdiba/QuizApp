<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ian.quizapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AndroidActivity"
            android:label="@string/androidName" />

        <activity android:name=".JavaActivity"
            android:label="@string/javaName"/>

        <activity android:name=".PythonActivity"
            android:label="@string/pythonName"/>

        <activity android:name=".WebActivity"
            android:label="@string/webName">

        </activity>

    </application>

</manifest>